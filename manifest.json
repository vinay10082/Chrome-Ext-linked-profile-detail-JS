{
    "name": "Task 2",
    "description": "Extension gives the linkedin profiles info",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions":[
        "activeTab",
        "scripting",
        "tabs"
    ],
    "action":{
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    }
}